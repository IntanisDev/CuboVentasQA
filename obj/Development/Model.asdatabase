          {
            "name": "UPT",
            "expression": "if('Retail Venta'[Transacciones]= 0 ,blank(),if (Venta[Unidades]=0, blank(),Venta[Unidades]/'Retail Venta'[Transacciones]))",
            "formatString": "#,0.00"
          },
          {
            "name": "AUR",
