                "",
                "    CASE WHEN len(rtrim(ltrim(Size))) < 3 THEN ",
                "       CASE WHEN len(rtrim(ltrim(Size))) = 1 THEN '00' + CONVERT(varchar, rtrim(ltrim(Size))) ",
                "       WHEN len(rtrim(ltrim(Size))) = 2 THEN '0' + CONVERT(varchar, rtrim(ltrim(Size))) END ",
                "       ELSE rtrim(ltrim(Size)) END AS Size, ",
                "",
                "    Large, ",
                "    StationYear, ",
                "    UPCCode, ",
                "    CASE WHEN ltrim(rtrim(isnull(PC13, ''))) = '' THEN NULL ELSE PC13 END AS PC13",
