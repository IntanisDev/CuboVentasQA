              "       ELSE rtrim(ltrim(Size)) END AS Size, ",
              "",
              "    Large, ",
              "    StationYear, ",
              "    UPCCode, ",
              "    CASE WHEN ltrim(rtrim(isnull(PC13, ''))) = '' THEN NULL ELSE PC13 END AS PC13",
              "",
              "FROM   (",
              "               SELECT DISTINCT ",
              "                     Product_4.ProductId, ",
              "                     Product_4.VariantCode, ",
              "                     Product_4.Variant, ",
              "                     Product_4.VariantGroup, ",
              "                     Product_4.Color, ",
              "                     Product_4.Size, ",
              "                     Product_4.Large, ",
              "                     Product_4.StationYear, ",
              "                     Product_4.UPCCode, ",
              "                     Product_4.PC13",
              "                 FROM   sales.Product AS Product_4 INNER JOIN sales.Sale ",
              "                              ON Product_4.ProductId = Sale.ProductId",
              "                  UNION",
              "                 ",
              "                 SELECT DISTINCT ",
              "                       ProductId, ",
              "                       VariantCode, ",
              "                       Variant, ",
              "                       VariantGroup, ",
              "                       Color, ",
              "                       Size, ",
              "                       Large, ",
              "                       StationYear, ",
              "                       UPCCode, ",
              "                       PC13",
              "                FROM  sales.Product AS Product_1",
              "                WHERE        (VariantCode = '0000')",
              "                   ",
              "                   UNION",
              "             ",
              "                SELECT DISTINCT ",
              "                      Product_3.ProductId, ",
              "                      Product_3.VariantCode, ",
              "                      Product_3.Variant, ",
              "                      Product_3.VariantGroup, ",
              "                      Product_3.Color, ",
              "                      Product_3.Size, ",
              "                      Product_3.Large, ",
              "                      Product_3.StationYear, ",
              "                      Product_3.UPCCode, ",
              "                      Product_3.PC13",
              "                 FROM  sales.Product AS Product_3 INNER JOIN sales.SaleABP ",
              "                             ON Product_3.ProductId = SaleABP.ProductId",
              "                          ",
              "                   UNION",
              "                   ",
              "                 SELECT DISTINCT ",
              "                       Product_2.ProductId, ",
              "                       Product_2.VariantCode, ",
              "                       Product_2.Variant, ",
              "                       Product_2.VariantGroup, ",
              "                       Product_2.Color, ",
              "                       Product_2.Size, ",
              "                       Product_2.Large, ",
              "                       Product_2.StationYear, ",
              "                       Product_2.UPCCode, ",
              "                       Product_2.PC13",
              "                  FROM  sales.Product AS Product_2 INNER JOIN sales.StoreCoverage ",
              "                              ON Product_2.ProductId = StoreCoverage.ProductId",
              "                    UNION",
              "                          ",
              "                   SELECT DISTINCT ",
              "                         Product_2.ProductId, ",
              "                         Product_2.VariantCode, ",
              "                         Product_2.Variant, ",
              "                         Product_2.VariantGroup, ",
              "                         Product_2.Color, ",
              "                         Product_2.Size, ",
              "                         Product_2.Large, ",
              "                         Product_2.StationYear, ",
              "                         Product_2.UPCCode, ",
              "                         Product_2.PC13",
              "                    FROM    sales.Product AS Product_2 INNER JOIN  sales.SellOut ",
              "                               ON Product_2.ProductId = SellOut.ProductId",
              ") AS Product</RSQueryCommandText><RSQueryCommandType>Text</RSQueryCommandType><RSQueryDesignState></RSQueryDesignState>]]></AnnotationContent></Gemini>"
            ]
